<h1>Edit Manage</h1>

<%= form_with model: @manage do |form| %>
  <div>
    <% @manage.errors.full_messages.each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :name %><br>
    <%= form.text_area :name %>
  </div>

   <div>
    <%= form.label :email %><br>
    <%= form.text_area :email %>
  </div>

  
  <div>
    <%= form.submit %>
  </div>
  
<% end %>